{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "PORT=5000 nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s client; npm run -s build; bash data/download_wordnet.sh",
    "test": "eslint src",
    "client": "babel src/client -s -D -d dist/client --presets es2015,stage-0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "browser": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "async": "^2.0.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "fluent-ffmpeg": "^2.1.0",
    "form-data": "^2.1.0",
    "fs": "0.0.1-security",
    "memory-streams": "^0.1.0",
    "natural": "^0.4.0",
    "node-cache": "^4.1.0",
    "node-uuid": "^1.4.7",
    "resource-router-middleware": "^0.6.0",
    "spellchecker": "^3.3.1",
    "wordnet-db": "^3.1.2",
    "wordnet-magic": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.10.2"
  }
}
